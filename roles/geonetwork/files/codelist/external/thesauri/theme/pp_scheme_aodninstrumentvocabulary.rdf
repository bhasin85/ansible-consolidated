  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L05/current/LAB39">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">Geiger counters</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <skos:inScheme rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-05-29T00:00:00Z</dcterms:created>
    <skos:definition xml:lang="en">Instruments that measure their distance above a specified elevation such as the sea surface or the seabed. Classification includes acoustic and pressure-based instruments that are designed to provide platform z co-ordinate spatial coverage in the atmosphere or a water body. It excludes remote sensing instruments such as LIDAR and satellite radar altimeters designed to map surface elevation.</skos:definition>
    <dc:source>(L051) SeaDataNet device categories</dc:source>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L05/current/LAB37">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">flow cytometers</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <skos:inScheme rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-03-28T00:00:00Z</dcterms:created>
    <skos:definition xml:lang="en">Instruments that suspend cells in a stream of fluid past detection sensors whilst illuminating them with laser light. Used for cell counting sorting biomarker detection and protein engineering.</skos:definition>
    <dc:source>(L051) SeaDataNet device categories</dc:source>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L05/current/110">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">wave recorders</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <skos:inScheme rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-03-28T00:00:00Z</dcterms:created>
    <skos:definition xml:lang="en">Instrument that measures water column surface wave parameters including height period direction and energy spectra.</skos:definition>
    <dc:source>(L051) SeaDataNet device categories</dc:source>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
