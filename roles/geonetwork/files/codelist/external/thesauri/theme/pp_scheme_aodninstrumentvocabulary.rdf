    <skos:definition xml:lang="en">Instruments measuring the relative levels of electromagnetic radiation of different wavelengths in the gamma-ray waveband.</skos:definition>
    <dc:source>(L051) SeaDataNet device categories</dc:source>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L05/current/69">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">neuston net</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <skos:inScheme rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-05-22T00:00:00Z</dcterms:created>
    <skos:definition xml:lang="en">A fine-meshed net designed to collect samples living within a few centimetres of the sea surface. Samples microzooplankton mesozooplankton and nekton.</skos:definition>
    <dc:source>(L051) SeaDataNet device categories</dc:source>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L05/current/68">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">multinet</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <skos:inScheme rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-05-22T00:00:00Z</dcterms:created>
    <skos:definition xml:lang="en">A system comprising several nets that can be opened and closed sequentially to collect a series of samples during a tow or haul. Samples microzooplankton mesozooplankton and nekton.</skos:definition>
    <dc:source>(L051) SeaDataNet device categories</dc:source>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L05/current/LAB20">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">spectrophotometers</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <skos:inScheme rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-05-22T00:00:00Z</dcterms:created>
    <skos:definition xml:lang="en">Instruments measuring the relative absorption of electromagnetic radiation of different wavelengths in the near infra-red visible and ultraviolet wavebands by samples.</skos:definition>
    <dc:source>(L051) SeaDataNet device categories</dc:source>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L05/current/LAB21">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">liquid scintillation counters</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <skos:inScheme rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-05-22T00:00:00Z</dcterms:created>
    <skos:definition xml:lang="en">Instruments assaying beta radiation by quantitative detection of visible light produced by the passage of rays or particles through a suitable scintillant incorporated into the sample.</skos:definition>
    <dc:source>(L051) SeaDataNet device categories</dc:source>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L05/current/WPS">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">water pressure sensors</skos:prefLabel>
    <skos:altLabel xml:lang="en" />
    <skos:inScheme rdf:resource="http://vocab.aodn.org.au/def/instrument/1" />
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-06-03T00:00:00Z</dcterms:created>
    <skos:definition xml:lang="en">Sensors measuring hydrostatic pressure that are capable of withstanding the physical demands made by in-situ measurements in water bodies. Depending upon the mode of deployment they may measure sea level, waves, mooring performance or the depth of a profiling instrument.</skos:definition>
    <dc:publisher>British Oceanographic Data Centre (BODC)</dc:publisher>
    <skos:narrower rdf:resource="http://vocab.nerc.ac.uk/collection/L22/current/TOOL0873/" />
  </rdf:Description>
  <rdf:Description rdf:about="http://vocab.nerc.ac.uk/collection/L22/current/TOOL0873/">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept" />
    <skos:prefLabel xml:lang="en">SMRU GPS Satellite Relay Data Logger (Argos)</skos:prefLabel>
